num1, num2, num3 = int (input ('Your number 1: ')), int (input ('Your number 2: ')), \
                   int (input ('Your number 3: '))
res_num1 = num1 >= num2 and num1 >= num3 and print('The largest number is', num1)
res_num2 = num2 >= num3 and num2 >= num1 and print('The largest number is', num2)
res_num3 = num3 >= num2 and num3 >= num1 and print('The largest number is', num3)
